[user]
	name = kasumi8pon
	email = all.splashing@gmail.com
[core]
	excludesfile = /Users/kasumi/.gitignore_global
	commentChar = ^
[alias]
	br = branch
	cm = commit -v
  cma = commit -v --amend
	co = checkout
	cp = cherry-pick
	lg = log --graph --all --decorate
  pushf = push --force-with-lease --force-if-includes
	rh = reset HEAD^
	sp = stash pop
	ss = stash save
	st = status
[init]
	defaultBranch = main
[pull]
	ff = only
